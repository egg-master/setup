{
	"name": "egg-setup",
	"displayName": "egg-setup",
	"description": "",
	"version": "0.0.3",
	"publisher": "egg",
	"engines": {
		"vscode": "^1.1.0"
	},
	"license": "MIT",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:template.update",
		"workspaceContains:egg/setup.json"
	],
	"main": "./out/extension",
	"contributes": {
		"commands": [
			{
				"command": "template.update",
				"title": "egg template update"
			},
			{
				"command": "develop.init",
				"title": "egg develop init"
			},
			{
				"command": "develop.clear",
				"title": "egg develop clear"
			},
			{
				"command": "develop.go",
				"title": "egg develop go"
			},
			{
				"command": "develop.goapp",
				"title": "egg develop goapp"
			},
			{
				"command": "develop.web",
				"title": "egg develop web"
			},
			{
				"command": "new.window",
				"title": "egg new window"
			},
			{
				"command": "new.test",
				"title": "egg new test"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test",
		"pack": "vsce package"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^7.0.43",
		"fs-extra": "^5.0.0",
		"jquery": "^3.3.1",
		"tslint": "^5.8.0",
		"typescript": "^2.6.1",
		"vscode": "^1.1.14"
	},
	"extensionDependencies": [
		"lukehoban.Go"
	],
	"dependencies": {}
}
